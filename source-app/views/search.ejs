<!DOCTYPE HTML>
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
		<link rel="stylesheet" type="text/css" href="css/detail.css" />
		<link rel="stylesheet" type="text/css" href="css/search.css" />
		<script src="https://ajax.lug.ustc.edu.cn/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
		<script src="https://ajax.lug.ustc.edu.cn/ajax/libs/angularjs/1.4.3/angular.min.js"></script>
	</head>
	<body ng-app="application" ng-controller="appController">
		<div class="form-wrapper">
			<input type="text" id="search" placeholder="样品编号..." ng-model="query_id" required>
			<input type="submit" value="搜索" id="submit" ng-click="querySource()">
		</div>
		<table cellspacing="0" id="query_source">
			<tr>
				<th>样品编号</th>
				<th>姓名</th>
				<th>出生日期</th>
				<th>C14</th>
				<th>C15</th>
				<th>C14</th>
				<th>C17</th>
				<th>C18</th>
				<th>C19</th>
			</tr>
			<tr>
          			<td>{{query_source.ObjName}}</td>
          			<td>{{query_source.ObjSpec}}</td>
          			<td>{{query_source.ObjMFGDate}}</td>
          			<td>{{query_source.ObjEXPDate}}</td>
		  		<td>{{query_source.ObjLOT}}</td>
		  		<td>{{query_source.ObjQSID}}</td>
		  		<td>{{query_source.ObjMFRSName}}</td>
		  		<td>{{query_source.ObjProPrice}}</td>
		  		<td>{{query_source.ObjProPlace}}</td>
        		</tr>
		</table>
	</body>
	<script type="text/javascript" src="/js/app.js"> </script>
</html>
